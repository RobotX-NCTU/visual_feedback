<launch>
  <arg name="veh" doc="Name of vehicle. ex: nibot"/>
  <arg name="local" defalut="false" doc="true for laptop. false for robot/>

  <arg name="pkg_name" default="visual_fb"/>
  <arg name="node_name" default="visual_feedback_node"/>

  <!-- run local -->
  <node if="$(arg local)" name="$(arg node_name)" pkg="$(arg pkg_name)" type="$(arg node_name).py" output="screen" clear_params="true" >
  </node>

  <!-- run remote -->

</launch>
